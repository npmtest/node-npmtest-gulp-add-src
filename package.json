        "gulp-add-src"
    ],
    "license": "MIT",
    "main": "lib.npmtest_gulp_add_src.js",
    "name": "npmtest-gulp-add-src",
    "nameAlias": "npmtest_gulp_add_src",
    "nameOriginal": "npmtest-gulp-add-src",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-gulp-add-src.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
